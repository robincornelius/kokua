<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<!-- Width and height of this panel should be synchronized with "panel_modes" in the floater_moveview.xml-->
<panel
 height="25"
 layout="topleft"
 name="panel_stand_stop_flying"
 mouse_opaque="false"
 visible="true"
 width="20">
    <button
     follows="left|bottom"
     height="18"
     label=""
     layout="topleft"
     left="2"
     name="stand_btn"
     image_overlay="Move_Walk_Off"
     image_selected="PushButton_Selected_Press"
     tool_tip="Click here to stand up."
     top="5"
     visible="false"
     width="18" />
    <button
     follows="left|bottom"
     height="18"
     label=""
     layout="topleft"
     left="2"
     name="stop_fly_btn"
     image_overlay="Move_Fly_Off"
     image_selected="PushButton_Selected_Press"
     tool_tip="Stop flying (press [Home] or F)"
     top="5"
     visible="false"
     width="18" />
</panel>
